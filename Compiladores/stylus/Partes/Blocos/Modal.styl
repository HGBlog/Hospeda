// CSS MODAL

.modal
	.modal-dialog
		.modal-content
			background-color CinzaPadrao
			border none
			border-radius 0
			.modal-header
				background-color AzulPadrao
				margin 0
				padding 9px 9px 4px 9px
				border-radius 0
				.modal-title
					color Branco
					text-transform none
					font-weight 400
					margin-bottom 10px
					margin-left 10px
				.close
					color Branco
					opacity 1
					margin-top -12px
					&:hover
						Hover()
			.modal-body
				background-color CinzaPadrao
				.SucessoFinalizar
					padding-left 41px
					padding-right 41px
					padding-top 42px
					padding-bottom 48px
					i
						font-size 127px
						color VerdePadrao
					.media-body						
						h5
							font-size 40px
							font-weight 500
							text-transform none
							top 9px
							position relative
							color VerdePadrao
							line-height 37px
						span
							top 4px
							position relative
							font-size 17px
					
			.modal-footer
				padding 9px 9px 10px 9px
				border-top 1px solid #dadada
				.btn-outline-success
					background-color VerdePadrao
					color Branco
					