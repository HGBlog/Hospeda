.navCncPadrao
	background-color AzulPadrao !important
	h1
	li
	a
		color Branco !important
	.navbar-brand
		margin-bottom 0
		small
			text-transform none
			font-size 21px
			position relative
			top -1px
			font-weight 400
	.BtnVoltar
		background-color AzulPadraoHover !important
		color Branco !important
		&:hover
			Hover()
			
	.justify-content-end
		.nav-item
			.nav-link
				padding 0 11px
				font-size 11px
				&:hover
					Hover()
				i
					display block
					font-size 227%
					text-align center
			.LinkPerfil
				background-image url(UrlImagensPastaUsuarios + 'user-example-40x40.jpg') 
				width 40px 
				height 40px
				font-size 0
				color transparent !important
				margin-left 13px
				margin-right 4px
			.BtnIcon
				i
					font-size 25px
					margin-top 7px
					margin-bottom -10px	
		