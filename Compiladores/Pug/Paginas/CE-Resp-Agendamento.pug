.row
	include ../Estrutura/CalendarioHorizontal
	
	// AQUI COMEÇA O BREADCRUMB DE AÇÕES
	.col-12.breadPadrao
		a.badge.badge-danger(href="#" title="Quem vai Agendar?")
			i.fas.fa-users
			| &nbsp; Quem vai agendar?
		
		span.float-right
			small.badge.badge-primary
				| Passo 1 
			| de 5
	// ------- FIM DO BREADCRUMB DE AÇÕES	

	// AQUI INICIA A PARTE DE CONTEÚDO GERAL
	.col-12.btOpcoesPadrao
		h2.display-4.text-center.text-uppercase
			| Quem vai agendar?
		.mx-auto.row
			.col-12.col-sm-6.col-md-4
				button(type="button", onClick= "window.location= 'Cad-Evento-P2.html'").btn.btn-lg.btn-block
					i.fas.fa-users.d-block
					| Equipe Nacional

			.col-12.col-sm-6.col-md-4
				button(type="button", data-toggle="modal" data-target="#ModalEquipeItinerante").btn.btn-lg.btn-block
					i.fas.fa-users.d-block
					| Equipe Itinerante

			.col-12.col-sm-6.col-md-4
				button(type="button", data-toggle="modal" data-target="#ModalEquipeLocal").btn.btn-lg.btn-block
					i.fas.fa-users.d-block
					| Equipe Local

			.col-12.col-sm-6.col-md-4
				button(type="button", data-toggle="modal" data-target="#ModalComunidadeLocal").btn.btn-lg.btn-block
					i.fas.fa-users.d-block
					| Comunidade Local

			.col-12.col-sm-6.col-md-4
				button(type="button", data-toggle="modal" data-target="#ModalContratante").btn.btn-lg.btn-block
					i.fas.fa-users.d-block
					| Contratante

		.mx-auto.row
			.col-12
				button.btn.btn-lg.btn-block.btnLine(type="button", data-toggle="modal" data-target="#ModalAgendarNada") 
					i.fas.fa-ban 
					| &nbsp; Não Agendar Nada Nesse Dia
	// ------- FIM DE CONTEÚDO GERAL