// MODAL DE COMUNIDADE LOCAL
#ModalAgendarNada.modal.fade(tabindex="-1", role="dialog", aria-labelledby="ModalComunidadeLocal", aria-hidden="true")
	.modal-dialog(role="document")
		.modal-content
			.modal-header
				h5#exampleModalLabel.modal-title Não Agendar Nada Nesse Dia!
				button.close(type="button", data-dismiss="modal", aria-label="Close")
					i.far.fa-times-circle(aria-hidden="true")
			.modal-body
				form(id="", method="", action="")
					.row
						.col
							label.col-form-label(for="inputDe") De
							input#inputDe.dtpicker.form-control(type='text', placeholder="00/00/0000", data-rangeto="#inputAte")
						.col
							label.col-form-label(for="inputAte") Até
							input#inputAte.dtpicker.form-control(type='text', placeholder="00/00/0000", data-rangefrom="#inputDe")
						
			.modal-footer
				button.btn.btn-primary(type="button", onClick= "window.location= 'index.html'") 
					i.far.fa-check-circle
					| &nbsp; Confirmar

				button.btn.btn-secondary(type="button", data-dismiss="modal") 
					i.fas.fa-ban 
					| &nbsp; Cancelar
// ------- FIM DO MODAL DE COMUNIDADE LOCAL