
// MODAL DE EQUIPE ITINERANTE
#ModalEquipeItinerante.modal.fade(tabindex="-1", role="dialog", aria-labelledby="ModalEquipeItinerante", aria-hidden="true")
	.modal-dialog(role="document")
		.modal-content
			.modal-header
				h5#exampleModalLabel.modal-title Equipe Itinerante
				button.close(type="button", data-dismiss="modal", aria-label="Close")
					i.far.fa-times-circle(aria-hidden="true")
			.modal-body
				form(id="", method="", action="")
					.form-group
						label.col-form-label(for="SelEquipeItinerante") Qual Equipe Está Agendando?
						select(name="SelEquipeItinerante").form-control.form-control-lg
							option(value="0")
								| Selecione
							option(value="1")
								| Exemplo de Equipe 1
							option(value="2")
								| Exemplo de Equipe 2
							option(value="3")
								| Exemplo de Equipe 3
			.modal-footer
				button.btn.btn-primary(type="button", onClick= "window.location= 'Cad-Evento-P2.html'") 
					i.far.fa-check-circle
					| &nbsp; Confirmar

				button.btn.btn-secondary(type="button", data-dismiss="modal") 
					i.fas.fa-ban 
					| &nbsp; Cancelar
// ------- FIM DO MODAL DE EQUIPE ITINERANTE



